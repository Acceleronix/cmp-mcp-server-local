# CMP API Configuration
# Copy this file to .env and fill in your actual values

# CMP API Key - Get this from your CMP dashboard
CMP_API_KEY=your_api_key_here

# CMP API Secret - Get this from your CMP dashboard  
CMP_API_SECRET=your_api_secret_here

# CMP API Endpoint - Default endpoint (you can change if needed)
CMP_API_ENDPOINT=https://cmp.acceleronix.io/gateway/openapi

# Optional defaults for pagination and formatting
DEFAULT_PAGE_SIZE=10
DEFAULT_FORMAT=compact
