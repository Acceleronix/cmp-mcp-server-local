{
	"name": "cmp-mcp-server",
	"type": "module",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"start": "node dist/index.js",
		"dev": "tsx watch src/index.ts",
		"serve": "tsx src/index.ts",
		"format": "biome format --write",
		"lint:fix": "biome lint --fix",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.13.0",
		"dotenv": "^16.4.5",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.4",
		"@types/node": "^22.10.1",
		"tsx": "^4.19.3",
		"typescript": "^5.8.3"
	}
}
